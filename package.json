{
  "name": "@awboost/cfn-resource-schemas-db",
  "version": "0.1.2",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "author": {
    "email": "gordonmleigh@gmail.com",
    "name": "Gordon Leigh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/awboost/cfn-resource-schemas-db"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "files": [
    "schemas/*.json",
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "lib/**/*.map",
    "!lib/generate/"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile && npm run lint",
    "clean": "rm -rf lib/",
    "compile": "tsc",
    "download": "node --loader=@swc-node/register/esm ./src/generate/commands/download.ts",
    "lint": "eslint src/ --ext=ts"
  },
  "dependencies": {
    "@awboost/cfn-resource-schemas": "^0.1.0"
  },
  "devDependencies": {
    "@swc-node/register": "^1.6.8",
    "@types/node": "^18.18.9",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "chalk": "^5.3.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "json-canonicalize": "^1.0.6",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  }
}
